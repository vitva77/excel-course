{
	"parser": "babel-eslint",
	"env": {
		"es6": true,
		"browser": true,
		"node": true
	},
	"rules": {
		"linebreak-style":"off",
		"object-curly-spacing":"off",
		"indent": "off",
		"no-tabs":"off",
		"array-bracket-spacing":"off",
		"comma-dangle":"off",
		"require-jsdoc":"off"
	},
	"extends": ["eslint:recommended", "google"]
}